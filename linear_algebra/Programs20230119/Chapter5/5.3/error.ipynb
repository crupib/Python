   "source": [
    "from numpy.linalg import det, matrix_rank\n",
    "from numpy.random import seed, normal\n",
    "\n",
    "seed(123)\n",
    "n = 20\n",
    "F =normal(0, 1, (n, n-1))\n",
    "G = F.dot(F.T)\n",
    "print(f'shape = {G.shape}')\n",
    "print(f'det = {det(G)}')\n",
    "print(f'rank = {matrix_rank(G)}')"
   ]
